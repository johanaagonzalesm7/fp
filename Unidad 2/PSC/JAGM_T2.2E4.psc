Proceso Promedio_de_calificaciones
	Definir C1,C2,C3,P Como Entero;
	Escribir ("Primer caligicacion:");
	Leer C1;
	Escribir ("Segundo calificacion:");
	Leer C2;
	Escribir ("Tercera calificacion:");
	Leer C3;
	P <- (C1+C2+C3)/3
	Si P>5 Entonces
		Escribir "Estas aprobado";
	Sino
		Escribir "Estas reprobado";
	FinSi
FinProceso
